{
  "applicationId": "js.neutralino.adminer_gui",
  "version": "1.0.0",
  "defaultMode": "window",
  "port": 0,
  "documentRoot": "/resources/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "logging": {
    "enabled": true,
    "writeToLogFile": true
  },
  "nativeBlockList": [],
  "globalVariables": {
    "TEST1": "Hello",
    "TEST2": [2, 4, 5],
    "TEST3": {
      "value1": 10,
      "value2": {}
    }
  },
  "modes": {
    "window": {
      "title": "Adminer GUI",
      "width": 450,
      "height": 300,
      "minWidth": 450,
      "minHeight": 300,
      "fullScreen": false,
      "alwaysOnTop": true,
      "icon": "/resources/icons/appIcon.png",
      "enableInspector": true,
      "borderless": true,
      "maximize": false,
      "hidden": false,
      "resizable": false,
      "exitProcessOnClose": true
    },
    "browser": {
      "globalVariables": {
        "TEST": "Test value browser"
      },
      "nativeBlockList": ["filesystem.*"]
    },
    "cloud": {
      "url": "/resources/#cloud",
      "nativeAllowList": ["app.*"]
    }
  },
  "cli": {
    "binaryName": "AdminerGUI",
    "resourcesPath": "/resources/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/resources/js/neutralino.js",
    "binaryVersion": "4.0.0",
    "clientVersion": "3.0.0"
  }
}
